{
  "type": "circle_button",
  "name": "circleButton",
  "icon": "home",
  "iconColor": "null",
  "iconSize": 25,
  "padding": "10",
  "margin": "10",
  "color": "Colors.green",
  "onPressed": "// run when press on this button\n",
  "onLongPress": "// run when long press on this button\n",
  "visible": true,
  "validator": {
    "version": 2,
    "name": {
      "type": "String",
      "regex": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "state": ""
    },
    "icon": {
      "type": "String",
      "regex": "^.*$",
      "state": "input"
    },
    "iconColor":{
      "type": "String|Color",
      "regex": "^(Colors\\.[a-z]*|Color\\(0x([0-9A-Fa-f]{8}\\))$",
      "state": "input"
    },
    "iconSize": {
      "type":  "Number",
      "regex": "^[0-9]{1,3}$",
      "state": "input"
    },
    "padding" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "margin" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "visible":{
      "type":  "Boolean",
      "regex": "^(true|false)$",
      "state": "input"
    }
  }
}