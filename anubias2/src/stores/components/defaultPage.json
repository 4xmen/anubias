{
  "type": "page",
  "name": "page",
  "scrollable": true,
  "safeArea": false,
  "padding": "15",
  "align": "null",
  "onShow": "// run when show page \n",
  "preview": "data:image/png;base64,",
  "children": {
    "visual": [],
    "nonVisual": []
  },
  "tie": [
  ],
  "validator": {
    "version": 2,
    "name" : {
      "type": "String",
      "regex": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "state": ""
    },
    "scrollable":{
      "type":  "Boolean",
      "regex": "/^(true|false)$/",
      "state": "input"
    },
    "safeArea":{
      "type":  "Boolean",
      "regex": "/^(true|false)$/",
      "state": "input"
    },
    "padding" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "align" : {
      "type": "String",
      "regex": "^(null|left|right|center)$",
      "state": "input"
    }
  }
}