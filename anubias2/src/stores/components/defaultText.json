{
  "type": "text",
  "name": "text",
  "text": "Anubias example text : )",
  "align": "null",
  "fontSize": 13.0,
  "height": 1.2,
  "maxLine": 0,
  "overflow": "null",
  "color": "null",
  "weight": "normal",
  "softWrap": true,
  "visible": true,
  "validator": {
    "version": 2,
    "name": {
      "type": "String",
      "regex": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "state": ""
    },
    "text": {
      "type": "String",
      "regex": ".*",
      "state": "input"
    },
    "fontSize": {
      "type":  "Number",
      "regex": "^\\d+(?:\\.\\d+)?$",
      "state": "input"
    },
    "height": {
      "type":  "Number",
      "regex": "^\\d+(?:\\.\\d+)?$",
      "state": "input"
    },
    "maxLine": {
      "type":  "Number",
      "regex": "^[0-9]{1,3}$",
      "state": "input"
    },
    "overflow": {
      "type": "String",
      "regex": "^(null|ellipsis|fade|visible|clip)$",
      "state": "input"
    },
    "color": {
      "type": "String|Color",
      "regex": "^(Colors\\.[a-z]*|Color\\(0x([0-9A-Fa-f]{8}\\))$",
      "state": "input"
    },
    "weight": {
      "type":  "String",
      "regex": "^FontWeight\\.w[0-9]{3}$",
      "state": "input"
    },
    "softWrap": {
      "type": "Boolean",
      "regex": "^(true|false)$",
      "state": "input"
    },
    "visible": {
      "type": "Boolean",
      "regex": "^(true|false)$",
      "state": "input"
    }
  }
}