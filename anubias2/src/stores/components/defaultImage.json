{
  "type": "image",
  "name": "image",
  "image": "",
  "padding": "0",
  "width": "100",
  "height": "null",
  "fit": "none",
  "align": "null",
  "online": false,
  "visible": true,
  "validator": {
    "version": 2,
    "name": {
      "type": "String",
      "regex": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "state": ""
    },
    "padding": {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "image": {
      "type": "String",
      "regex": "^.*$",
      "state": "input"
    },
    "width" : {
      "type": "String|Number",
      "regex":  "^\\d*\\.?\\d+%?$",
      "state": "input"
    },
    "height" : {
      "type": "String|Number",
      "regex":  "^\\d*\\.?\\d+%?$",
      "state": "input"
    },
    "fit" : {
      "type": "String|Select",
      "regex": "^BoxFit\\.[a-zA-Z_]*$",
      "state": "input",
      "Items": ["BoxFit.fill", "BoxFit.contain", "BoxFit.cover", "fitWidth", "fitHeight", "none", "scaleDown"]
    },
    "align" : {
      "type": "String|Align",
      "regex": "^(null|left|right|center)$",
      "state": "input"
    },
    "online": {
      "type": "Boolean",
      "regex": "^(true|false)$",
      "state": "input"
    },
    "visible": {
      "type": "Boolean",
      "regex": "^(true|false)$",
      "state": "input"
    }
  }
}