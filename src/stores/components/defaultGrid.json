      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "borderColor":{
      "type": "String|Color",
      "regex": "^(Colors\\.[a-z]*|Color\\(0x([0-9A-Fa-f]{8}\\))$",
      "state": "input"
    },
    "padding" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "margin" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "borderRadius" : {
      "type": "String|Number",
      "regex": "^\\d+(,\\d+){0,3}$",
      "state": "input"
    },
    "childAspectRatio": {
      "type": "String",
      "regex": "^[0-9]{1,2}$",
      "state": "input"
    },
    "visible": {
